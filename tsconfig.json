{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "exclude": [
    "jest.config.js"
  ],
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@modules/*": [
        "./src/modules/*"
      ],
      "@locale/*": [
        "./src/locale/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@themes/*": [
        "./src/themes/*"
      ],
    }
  }
}
